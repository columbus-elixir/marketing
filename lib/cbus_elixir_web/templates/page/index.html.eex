        <h2 class="title myriad">This Month's Speakers</h2>
        <ul class="speakers">
            <%= Enum.map(@speakers, fn(speaker) -> %>
                <li>
                    <a href="#">
                        <img src='<%= gravatar_url(speaker.email) %>', alt="Speaker Image" />
                        <div class="details">
                          <div class="name"><%= speaker.name %></div>
                          <div class="talk-name"><%= speaker.title %></div>
                        </div>
                    </a>
                </li>
            <% end) %>
        </ul>
    </div>

